<%= form_with url: "/teams/#{@team.id}/players", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :age %>
  <%= form.text_field :age %>

  <%= form.label :retired? %>
  <%= form.text_field :retired %>

  <%= form.submit "Create Player" %>
<% end %>
